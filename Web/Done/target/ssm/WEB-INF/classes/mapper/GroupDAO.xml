<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.done.dao.GroupDAO">
    <select id="queryByID" resultType="Group" parameterType="String">
		select * from `group` where groupID = #{groupID}
    </select>

    <select id="queryAll" resultType="Group">
		select * from `group`
	</select>

</mapper>