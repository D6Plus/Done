<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.done.dao.PlanDAO">
    <select id="queryByID" resultType="Plan" parameterType="String">
		select * from `plan` where planID = #{planID}
    </select>

    <select id="queryAll" resultType="Plan">
		select * from `plan`
	</select>

</mapper>